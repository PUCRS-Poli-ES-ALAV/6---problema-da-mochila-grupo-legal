from dataclasses import dataclass
from typing import List

@dataclass
class Item:
    weight: int
    value: int


def main():
    capacity: int = 190
    items: List[Item] = [
        Item(56, 50),   
        Item(59, 50),
        Item(80, 64),
        Item(64, 46),
        Item(75, 50),
        Item(17, 5)
    ]

    print(run(capacity, items))


def run(capacity: int, items: List[Item]) -> List[Item]:
    if sum(i.weight for i in items) <= capacity:
        return items

    branches = [run(capacity, [i for i in items if i != item]) for item in items]

    max_value = 0
    max_branch = []

    for branch in branches:
        total_value = sum(i.value for i in branch)

        if total_value > max_value:
            max_value = total_value
        max_branch = branch

    return max_branch


def load_items():
    pass


if __name__ == '__main__':
    main()
